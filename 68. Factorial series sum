#include <iostream>
using namespace std;
int main() {
    int n = 5;
    int sum = 0;
    for(int i = 1; i <= n; i++) {
        int fact = 1;
        for(int j = 1; j <= i; j++) {
            fact *= j; 
        }
        sum += fact / i; 
    }
    cout << "Sum of the series: " << sum << endl;
    return 0;
}
