#include <iostream>
using namespace std;

int main() {
    int num, original, reversed = 0, digit;
    
    cout << "Enter a number: ";
    cin >> num;
    
    original = num;
    
    while (num > 0) {
        digit = num % 10;
        reversed = reversed * 10 + digit;
        num /= 10;
    }
    
    if (original == reversed) {
        cout << "It is a Palindrome" << endl;
    } else {
        cout << "It is not a Palindrome" << endl;
    }

    return 0;
}
#include <iostream>
using namespace std;

int main() {
    int num, original, reversed = 0, digit;
    
    cout << "Enter a number: ";
    cin >> num;
    
    original = num;
    
    while (num > 0) {
        digit = num % 10;
        reversed = reversed * 10 + digit;
        num /= 10;
    }
    
    if (original == reversed) {
        cout << "It is a Palindrome" << endl;
    } else {
        cout << "It is not a Palindrome" << endl;
    }

    return 0;
}
