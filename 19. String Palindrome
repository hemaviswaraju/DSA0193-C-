#include<iostream>
using namespace std;
int main() {
    string a;
    cout<<"Enter the string:";
    cin>>a;
    string rev="";
    int n=a.length();
    for(int i=n-1;i>=0;i--){
        rev+=a[i];
    }
    cout<<rev;
    if(a==rev){
        cout<<"\nPalindrome";
    }else{
        cout<<"\nNot a palindrome";
    }
    return 0;
}
