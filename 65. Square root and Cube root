#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double num;
    
    cout << "Enter a number to find the square root: ";
    cin >> num;
    cout << "Square root: " << sqrt(num) << endl;
    
    cout << "Enter a number to find the cube root: ";
    cin >> num;
    cout << "Cube root: " << cbrt(num) << endl;
    
    return 0;
}
