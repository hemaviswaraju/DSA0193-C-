#include<iostream>
using namespace std;

double ToFahrenheit(double celsius) {
    return (celsius * 9 / 5) + 32;
}

int main() {
    double celsius[] = {4.4, 10, 12, 7, 70};
    int n = sizeof(celsius) / sizeof(celsius[0]);
    cout << "Fahrenheit values: ";
    for (int i = 0; i < n; i++) {
        cout << ToFahrenheit(celsius[i]) << " ";
    }
    return 0;
}
